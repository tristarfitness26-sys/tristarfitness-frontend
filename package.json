{
  "name": "tristar-fitness",
  "version": "1.0.0",
  "description": "Tri Star Fitness - Professional Gym Management System",
  "type": "module",
  "main": "index.html",
  "homepage": "https://nishchaydev.github.io/tristar-fitness-clean/",
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "dev": "vite",
    "dev:server": "cd backend && npm run dev",
    "devall": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "vite build && npm run _postbuild:copy404",
    "_postbuild:copy404": "node -e \"const fs=require('fs');fs.copyFileSync('dist/index.html','dist/404.html');console.log('Copied 404.html');\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "git": "node scripts/deploy.js",
    "start": "npm run devall",
    "build:prod": "npm run build && cd backend && npm install --production",
    "start:prod": "cd backend && NODE_ENV=production npm start",
    "_deprecated:electron": "echo Electron removed"
  },
